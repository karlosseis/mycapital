        </tr>         
        <tr>
            <td style="text-align:left"><%= t(:'simple_form.labels.company_historic_dividend.debt') %></td>
            <td style="text-align:right"><strong><%= number_to_human(@company.stats('debt'), precision: 5) %></strong></td> 
        </tr>  


    </tbody>
    </table>

  </div>


  <div class="col-md-4">
