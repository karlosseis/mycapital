        </tr> 
        <tr>
            <td style="text-align:left"><%= t(:'simple_form.labels.company_historic_dividend.revenue') %></td>
            <td style="text-align:right"><strong><%= number_to_human(@company.stats('revenue'), precision: 5) %></strong></td> 
        </tr>         
        <tr>
            <td style="text-align:left"><%= t(:'simple_form.labels.company_historic_dividend.grossProfit') %></td>
            <td style="text-align:right"><strong><%= number_to_human(@company.stats('grossProfit'), precision: 5) %></strong></td> 
        </tr>         
        <tr>
            <td style="text-align:left"><%= t(:'simple_form.labels.company_historic_dividend.EBITDA') %></td>
