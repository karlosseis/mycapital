<h1>Listing Yahoo Tickers</h1>

<table id="myTable" class="table  table-striped">  
  <thead>
    <tr>
      <th>Ticker</th>
      <th>Name</th>
      <th>Exchange</th>
      <th>Name country</th>
      <th>Category</th>
      <th></th>
      <th></th>
      <th></th>
      th></th>
    </tr>
  </thead>

  <tbody>
    <% @yahoo_tickers.each do |yahoo_ticker| %>
      <tr>
        <td><%= yahoo_ticker.ticker %></td>
        <td><%= yahoo_ticker.name %></td>
        <td><%= yahoo_ticker.exchange %></td>
        <td><%= yahoo_ticker.name_country %></td>
        <td><%= yahoo_ticker.category %></td>
        <td><%= link_to 'Crear empresa', create_company %></td>

        <td><%= link_to 'Show', yahoo_ticker %></td>
        <td><%= link_to 'Edit', edit_yahoo_ticker_path(yahoo_ticker) %></td>
        <td><%= link_to 'Destroy', yahoo_ticker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

