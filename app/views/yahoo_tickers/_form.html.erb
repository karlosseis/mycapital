<%= simple_form_for(@yahoo_ticker) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :ticker %>
    <%= f.input :name %>
    <%= f.input :exchange %>
    <%= f.input :name_country, as: :text %>
    <%= f.input :category %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
