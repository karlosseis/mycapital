<p id="notice"><%= notice %></p>

<h1>Listing Company Historic Dividends</h1>

<table>
  <thead>
    <tr>
      <th>Exdividend date</th>
      <th>Record date</th>
      <th>Announce date</th>
      <th>Payment date</th>
      <th>Amount</th>
      <th>Float</th>
      <th>Dividend type</th>
      <th>Company</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @company_historic_dividends.each do |company_historic_dividend| %>
      <tr>
        <td><%= company_historic_dividend.exdividend_date %></td>
        <td><%= company_historic_dividend.record_date %></td>
        <td><%= company_historic_dividend.announce_date %></td>
        <td><%= company_historic_dividend.payment_Date %></td>
        <td><%= company_historic_dividend.amount %></td>
        <td><%= company_historic_dividend.float %></td>
        <td><%= company_historic_dividend.dividend_type %></td>
        <td><%= company_historic_dividend.company %></td>
        <td><%= company_historic_dividend.user %></td>
        <td><%= link_to 'Show', company_historic_dividend %></td>
        <td><%= link_to 'Edit', edit_company_historic_dividend_path(company_historic_dividend) %></td>
        <td><%= link_to 'Destroy', company_historic_dividend, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

