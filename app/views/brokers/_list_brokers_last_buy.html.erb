
    <% @next_dates_to_buy.each  do |key, value| %>


        <dl class="uk-description-list">
                  <dt><%= key %></dt>
                  <dd>
                <% if value >= Time.new + 10.day %>        
                <% else %>
                    <font color="#ff0000"><%= format_date(value)  %></font>
                <% end %>     
                </dd>
        </dl>
      
        


     
    <% end %>


