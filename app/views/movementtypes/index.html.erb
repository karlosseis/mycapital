<p id="notice"><%= notice %></p>

<h1>Listing Movementtypes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movementtypes.each do |movementtype| %>
      <tr>
        <td><%= movementtype.name %></td>
        <td><%= movementtype.user %></td>
        <td><%= link_to 'Show', movementtype %></td>
        <td><%= link_to 'Edit', edit_movementtype_path(movementtype) %></td>
        <td><%= link_to 'Destroy', movementtype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Movementtype', new_movementtype_path %>
